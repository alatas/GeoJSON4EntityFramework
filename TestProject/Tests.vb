    <TestMethod()> Public Sub TestGeometryCollection()
        Dim geom = DbGeometry.FromText("GEOMETRYCOLLECTION(POINT (30 10), POLYGON ((30 10, 40 40, 20 40, 10 20, 30 10)))", 4326)
        Dim geomc = CType(GeometryCollection.FromDbGeometry(geom), GeometryCollection)

        Assert.IsNotNull(geomc)
        Assert.AreEqual(2, geomc.Geometries.Count)
        Assert.IsInstanceOfType(geomc.Geometries(0), GetType(Point))
        Assert.IsInstanceOfType(geomc.Geometries(1), GetType(Polygon))
    End Sub

